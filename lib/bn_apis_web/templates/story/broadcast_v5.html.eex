<html><head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <style>
        .body {
            width: 100%;
            height: 100%;
            overflow: hidden;
            scroll: none;
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
            margin: 0;
            padding: 0;
            border: 0;
            font-family: sans-serif;
        }
        .bn-brand-logo {
            position: relative;
            width: 100%;
            top: 30px;
            left: 20px;
            height: 28px;
        }
        .container {
            bottom: 20px;
            color: #fff;
            background: #000;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: auto;
            height: 100vh;
        }
        .bn-bottom-txt {
            height: 40px;
        }

        .button {
            text-transform: uppercase;
            background-color: white;
            color: #000000;
            font-size: 14px;
            font-weight: 600;
            padding: 10px;
            text-align: center;
            border-radius: 10px;
            position:absolute;
            right: 0;
            bottom: 0;
            width:120px;
            margin: 20px;
        }

        .sub-header {
            font-size: 14px;
            color: white;
            font-weight: 600;
            margin-bottom: 30px;
        }

        .bn-lg-div {
            display: inline-block;
            vertical-align: top;
        }
        .bn-exclusive {
            font-size: 8px;
            font-weight: 600;
            padding: 4px 6px 2px 6px;
            border-radius: 5px;
            color: #0a0a1e;
            background-color: white;
            display: inline-block;
            vertical-align: top;
            top: 5px;
            left: 5px;
            position: relative;
        }
        .bn-main-contents {
            display: table;
            height: 100%;
            padding: 0 20px 150px;
            color: #ffffff;
        }
        .bn-invite-title {
            display: table-cell;
            vertical-align: middle;
            line-height: 50px;
            font-size: 30px;
        }
        .bn-txt-btm {
            margin-bottom: 60px;
            font-size: 17px;
            font-weight: 700;
        }
        .bn-ib {
            display: inline-block;
            vertical-align: top;
        }
        .bn-loc {
            width: 260px;
            left: 10px;
            position: relative;
            line-height: 25px;
        }
        .bn-loc-ic {
            position: relative;
            top: 5px;
        }
        #video1 {
          max-height: 80%;
        }
      </style>
  </head>
  <body class="body">
   <div class="container">
     <video id="video1" width="100%" autoplay>
      <source src="https://panel.brokernetwork.app/static/cab_ad.mp4" type="video/mp4">
      Your device does not support HTML video.
    </video>
    <div class="button" onClick="test();">Interested</div>
   </div>
   <script>
        function test() {
            JSInterface.submitEvent('SV 300',JSON.stringify({'UPDATE_CLICKED': 'Yes'}));
            JSInterface.openApp()
            JSInterface.finish();
        }
    </script>
</body></html>
