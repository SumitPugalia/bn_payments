<html>
    <style>
        @font-face {
            font-family: "Graphik";
            src: url(https://unpkg.com/@anarock/pebble@0.10.0/dist/fonts/anarock-regular.woff) format("woff"),
            url(https://unpkg.com/@anarock/pebble@0.10.0/dist/fonts/anarock-regular.woff2) format("woff2"),
            url(https://unpkg.com/@anarock/pebble@0.10.0/dist/fonts/anarock-regular.ttf) format("truetype"),
            url(https://unpkg.com/@anarock/pebble@0.10.0/dist/fonts/anarock-regular.svg#Anarock) format("svg");
            font-style: normal;
            font-weight: normal;
        }
        .top-wrap {
            background:url('data:image/svg+xml;base64,');
            width: 100%;
            height: 330px;
            background-repeat: no-repeat;
            background-size: cover;
            position: absolute;
            top: 0;
        }
        .tag {
            position: relative;
            top:30px;
            left: 20px;
            margin: 40px;
            background-color:#171726;
            color: white;
            border-radius: 10px;
            width: 270px;
            height: 60px;
            font-size: 24px;
            font-weight: 500;
            text-align: center;
            line-height: 60px;
        }
        .bottom-wrap {
            position: absolute;
            top: 170px;
            text-align: center;
            width: 700px;
        }
        .very-specific-design {
            width: 700px;
            height: 1070px;
            position: relative;
            top: <%= @broker_data["div_top"] %>px;
            -ms-transform: scale(<%= @broker_data["scale"] %>) ;
            -webkit-transform: scale(<%= @broker_data["scale"] %>);
            -moz-transform: scale(<%= @broker_data["scale"] %>);
            transform: scale(<%= @broker_data["scale"] %>);
            -ms-transform-origin: top center;
            -webkit-transform-origin: top center;
            -moz-transform-origin: top center;
            transform-origin: top center;
            border-radius: 30px;
            box-shadow: 0px 0px 30px rgba(0,0,0,0.35);
            overflow: hidden;
            margin: 0 auto;
        }

        .scaleable-wrapper {
            padding: 0;
            resize: both;
            position: relative;
            height: 100%;
            width: 100%;
            margin: 0 auto;
        }

        .img-sr{
            display: inline-block;
            vertical-align: top;
            width: 300px;
            height: 300px;
            border-radius: 50%;
            background-size: cover;
            background-repeat: no-repeat;
            border: 8px solid #ffffff;
            background-image: url(<%= raw @broker_data["profile_pic_url"] %>);
            top: 10px;
            position: relative;
            box-shadow: 5px 5px 15px rgba(0,0,0,0.15);
        }
        .txt-content {
            display: inline-block;
            vertical-align: top;
            width: 580px;
            height: 200px;
            top: 10px;
            position: relative;
            left: 40px;
        }
        .call-img {
            background: url('data:image/svg+xml;base64,'); height: 36px; width: 36px;margin-top: 10px; background-repeat: no-repeat; background-size: cover;display: inline-block;margin-right: 15px;
        }
        *{
            font-family: Graphik, sans-serif;
        }
        a{
            text-decoration: none;
        }
    </style>


<body id='scl-bdy'>
        <div class="scaleable-wrapper" id="scaleable-wrapper">
  
                <div class="very-specific-design" id="very-specific-design">
                    <div class="top-wrap">
                        <div class="tag">CHANNEL PARTNER</div>
                    </div>
                    <div class="bottom-wrap">
                        <div class="img-sr"></div>
                        <div style="margin-top: 50px;font-size: 48px;line-height: 64px;text-align: center;padding: 0px 30px;margin-bottom: 20px;font-weight: 700;font-family: Graphik, sans-serif;width:640px;word-break: break-word;"><%= @broker_data["name"] %></div>
                        <div style="color:#6e6e7a;font-size: 32px;line-height: 48px;padding: 0px 30px;text-align: center;"><%= @broker_data["organization_name"] %></div>
                        <a href="tel:<%= @phone_number %>" style="display: block;height:320px;color:#000000;font-family: Graphik, sans-serif;font-size:42px;margin-top: 60px; text-align: center;font-weight: 700"><span class="call-img"></span><%= @phone_number %><div style="display: block;background-color: black; color: white; margin: 60px auto 0px; width: 300px;padding: 30px; border-radius: 150px;text-align: center;font-size: 32px;">Call</div></a>
                    </div>
        
                </div>

            </div>
</body>
</html>
